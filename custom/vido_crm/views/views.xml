<odoo>
    <!-- view body -->
    <record id="student_view_tree" model="ir.ui.view">
      <field name="name">student.tree</field>
      <field name="model">student</field>
      <field name="arch" type="xml">
        <tree string="">
          <field name="Name" />
          <field name="MSSV" />
          <field name="Ngaysinh" />
          <field name="Thangsinh" />
          <field name="Namsinh" />
          <field name="Gender" />
          <field name="SDT" />
          <field name="Status" />
          <field name="UpdateDatetime" />
          <field name="ResponseStatus" />
        </tree>
      </field>
    </record>

    <record id="student_form" model="ir.ui.view">
      <field name="name">student.form</field>
      <field name="model">student</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
                <group>
                    <field name="Name" />
                    <field name="MSSV" />
                    <field name="Gender" />
                    <field name="SDT" />
                </group>
                <group>
                  <field name="Ngaysinh" />
                  <field name="Thangsinh" />
                  <field name="Namsinh" />
                  <field name="Status" />
                  <field name="UpdateDatetime" />
                  <field name="ResponseStatus" />
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record id="student_action" model="ir.actions.act_window">
      <field name="name">vidoClass</field>
      <field name="res_model">student</field>
      <field name="view_mode">tree,form</field>
      <field name="domain">[]</field>
      <field name="context">{}</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
          create student!
        </p>
      </field>
    </record>
    

  <menuitem
    id="Class_menu"
    name="VidoCRM"
    sequence="0"/>
  
  <menuitem
    id="Student_Class_root"
    name="Student"
    parent="Class_menu"
    sequence="10"/>
  <menuitem
    id="Piperline_Student_root"
    name="My model"
    parent="Student_Class_root"
    action="student_action"
    sequence="10"/>
  
</odoo>