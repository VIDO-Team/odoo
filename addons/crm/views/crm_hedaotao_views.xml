<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hedaotao_view_tree" model="ir.ui.view">
        <field name="name">hedaotao.tree</field>
        <field name="model">crm.hedaotao</field>
        <field name="arch" type="xml">
          <tree string="">
            <field name="Ma" />
            <field name="Ten" />
            <field name="TenTA" />
            <field name="HinhthucdaotaoId" />
            <field name="kyHieu" />
          </tree>
        </field>
      </record>
      <record id="hedaotao_form" model="ir.ui.view">
        <field name="name">hedaotao.form</field>
        <field name="model">crm.hedaotao</field>
        <field name="arch" type="xml">
          <form string="">
            <sheet>
              <group>
                  <group>
                    <field name="Ma" />
                    <field name="Ten" />
                  </group>
                  <group>
                    <field name="TenTA" />
                    <field name="HinhthucdaotaoId" />
                    <field name="kyHieu" />
                </group>
              </group>
            </sheet>
          </form>
        </field>
      </record>

    <record id="hedaotao_action" model="ir.actions.act_window">
        <field name="name">hedaotaoVido</field>
        <field name="res_model">crm.hedaotao</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            create hedaotao!
          </p>
        </field>
      </record>

</odoo>
