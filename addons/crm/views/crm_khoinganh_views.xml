<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="khoinganh_view_tree" model="ir.ui.view">
        <field name="name">khoinganh.tree</field>
        <field name="model">crm.khoinganh</field>
        <field name="arch" type="xml">
          <tree string="">
            <field name="Ma" />
            <field name="Ten" />
            <field name="ChuyenNganhId" />
            <field name="KhoaHoc" />
            <field name="SoTinChi" />
            <field name="SoHocKy" />
          </tree>
        </field>
      </record>
      <record id="khoinganh_form" model="ir.ui.view">
        <field name="name">khoinganh.form</field>
        <field name="model">crm.khoinganh</field>
        <field name="arch" type="xml">
          <form string="">
            <sheet>
              <group>
                  <group>
                    <field name="Ma" />
                    <field name="Ten" />
                    <field name="ChuyenNganhId" />
                  </group>
                  <group>
                    <field name="KhoaHoc" />>
                    <field name="SoTinChi" />
                    <field name="SoHocKy" />
                </group>
              </group>
            </sheet>
          </form>
        </field>
      </record>

    <record id="khoinganh_action" model="ir.actions.act_window">
        <field name="name">KhoiNganhVido</field>
        <field name="res_model">crm.khoinganh</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            create khoinganh!
          </p>
        </field>
      </record>

</odoo>